{
  "name": "app-nehmen",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "tsc": "node_modules/.bin/tsc",
    "rollup": "node_modules/.bin/rollup",
    "rimraf": "node_modules/.bin/rimraf",
    "start": "ng serve --serve-path=/app-nehmen --base-href=/app-nehmen --deploy-url=/app-nehmen",
    "start-prod": "npm run build && http-server -p 8080 -c-1 dist",
    "build": "npm run sw-build && ng build --prod",
    "sw-clean": "npm run rimraf -- dist-sw",
    "sw-compile": "npm run tsc -- --build src/tsconfig.sw.json",
    "sw-rollup": "npm run rollup -- --config src/service-worker/rollup.config.js",
    "sw-build": "npm run sw-clean && npm run sw-compile && npm run sw-rollup",
    "build-gh": "npm run sw-build && ng build --prod --base-href \"https://www.tkglaser.net/app-nehmen/\"",
    "deploy-gh": "ngh --no-silent",
    "test": "ng test",
    "test-ci": "ng test --no-progress --no-watch --browsers=ChromiumHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/pwa": "^0.12.1",
    "@angular/router": "~7.2.0",
    "@angular/service-worker": "~7.2.0",
    "core-js": "^2.6.1",
    "dropbox": "^4.0.14",
    "hammerjs": "^2.0.8",
    "idb": "^2.1.3",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.23.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.2.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jasmine": "~2.8.14",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.2",
    "rimraf": "^2.6.3",
    "rollup": "^0.68.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
